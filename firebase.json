{
  "functions": {
    "source": "functions",
    "runtime": "nodejs20",
    "ignore": ["node_modules", ".git", "firebase-debug.log"]
  },
  "hosting": {
    "site": "entrestate-com",
    "public": "public",
    "ignore": ["**/.*", "**/node_modules/**"],
    "rewrites": [
      { "source": "**", "function": "nextServer" }
    ]
  },
  "emulators": {
    "functions": { "port": 5001 },
    "hosting": { "port": 5000 },
    "ui": { "enabled": true }
  }
}