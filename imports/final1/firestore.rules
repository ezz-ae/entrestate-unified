rules_version = '2';
service cloud.firestore { match /databases/{database}/documents {
  match /{document=**} { allow read, write: if false; }
  match /projects_catalog/{docId} { allow read: if true; allow write: if false; }
  match /users/{uid}/{document=**} { allow read, write: if request.auth != null && request.auth.uid == uid; }
  match /marketData/{docId} { allow read: if true; allow write: if false; }
}}